<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.daggerwork.chat.runner">

    <application
        android:name="com.example.android.daggerwork.chat.runner.ChatRunnerApp"
        android:theme="@style/Theme.DaggerWork">

        <activity android:name="com.example.android.daggerwork.chat.runner.ChatRunnerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
